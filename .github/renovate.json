{
  "commitMessageAction": "Update",
  "commitMessageTopic": " {{depName}}",
  "packageRules": [
    {
      "matchManagers": [
        "sbt"
      ],
      "enabled": false
    },
    {
      "matchManagers": [
        "docker-compose"
      ],
      "matchPackagePatterns": [
        "elasticsearch"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "enabled": false
    }
  ],
  "rebaseWhen": "conflicted",
  "schedule": [
    "after 6pm every workday",
    "every weekend"
  ]
}
